{
  "name": "alex-patterson-webdev/factory",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Alex Patterson",
      "email": "alex.patterson.webdev@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.3",
    "psr/container": "^1.0.0"
  },
  "require-dev" : {
    "phpunit/phpunit" : "^9.1"
  },
  "autoload": {
    "psr-4": {
      "Arp\\Factory\\" : "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ArpTest\\Factory\\" : "test/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
